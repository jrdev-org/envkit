{
  "name": "envkit",
  "version": "1.0.0",
  "description": "Envkit is a secure environment variable management tool for modern teams. It allows you to store and manage environment variables in a secure and organized way, with built-in encryption and access control.",
  "type": "module",
  "scripts": {
    "dev:web": "pnpm --filter @envkit/web dev",
    "dev:cli": "pnpm --filter @envkit/cli dev",
    "dev:db": "pnpm --filter @envkit/db dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "dev:all": "pnpm --parallel dev",
    "test": "vitest run",
    "test:web": "vitest run tests/web",
    "test:cli": "vitest run tests/cli",
    "test:db": "vitest run tests/db",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "keywords": [
    "envkit",
    "env",
    "environment variables",
    "secrets",
    "security",
    "encryption",
    "access control",
    "team",
    "securem anagement tool",
    "modern",
    "devops"
  ],
  "author": {
    "name": "Envkit",
    "email": "envkit@envkit.sh",
    "url": "https://envkit.sh"
  },
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "dotenv": "^17.2.2",
    "tsx": "^4.20.5",
    "vitest": "^3.2.4"
  }
}
