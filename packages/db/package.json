{
  "name": "@envkit/db",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./env": {
      "import": "./dist/src/env.js",
      "types": "./dist/src/env.d.ts"
    },
    "./encryption": {
      "import": "./dist/src/encryption.js",
      "types": "./dist/src/encryption.d.ts"
    },
    "./types": {
      "import": "./dist/convex/_generated/dataModel.js",
      "types": "./dist/convex/_generated/dataModel.d.ts"
    }
  },
  "files": [
    "dist",
    "convex/_generated",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "convex dev",
    "deploy": "convex deploy",
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp -r convex dist/"
  },
  "keywords": [
    "convex",
    "envkit"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@t3-oss/env-core": "^0.13.8",
    "convex": "^1.27.0",
    "dotenv": "^17.2.2",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "tsx": "^4.20.5"
  }
}
